#!/usr/bin/env bash
set -e

bin/build
set -e
if [ "$#" == "0" ]; then
  bin/cuke-api
  bin/cuke-cli
else
  DEBUG='*,-express:*,-body-parser:*,-observable-process,-text-stream-search,-rails-delegate,-exorelay:*,-stylus:*' bin/cuke-api "$@"
  DEBUG='*,-express:*,-body-parser:*,-observable-process,-text-stream-search,-rails-delegate,-exorelay:*,-stylus:*' bin/cuke-cli "$@"
fi
