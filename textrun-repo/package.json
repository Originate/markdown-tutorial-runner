{
  "name": "textrun-repo",
  "version": "0.1.2",
  "description": "Text-Runner actions for verifying the content of the current repository",
  "license": "ISC",
  "type": "module",
  "exports": "./dist/index.js",
  "files": [
    "dist/**/*.js"
  ],
  "scripts": {
    "build": "tsc -p tsconfig-build.json",
    "clean": "rm -rf dist",
    "cuke": "cucumber-js --format=progress-bar",
    "doc": "text-run static --format=summary && text-run dynamic --format=progress",
    "fix": "eslint . --fix --ext .ts --ignore-path ../.eslintignore && prettier --write .",
    "lint": "prettier -l . && eslint . --ext .ts --ignore-path ../.eslintignore && depcheck --config=../.depcheckrc"
  },
  "dependencies": {
    "assert-no-diff": "3.0.6",
    "colorette": "2.0.19",
    "eol": "0.9.1",
    "text-runner-core": "5.0.2"
  },
  "devDependencies": {
    "shared-cucumber-steps": "0.0.0",
    "text-runner": "5.0.2",
    "textrun-action": "0.1.2",
    "textrun-extension": "0.1.2",
    "textrun-npm": "0.1.2",
    "ts-node": "10.9.1"
  }
}
