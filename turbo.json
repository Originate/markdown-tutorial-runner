{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["src/*.ts", "src/**/*.ts", ":!:src/*.test.ts", ":!:src/**/*.test.ts", "tsconfig.json", "package.json"],
      "outputs": ["dist/"],
      "outputMode": "new-only"
    },
    "clean": {
      "outputs": []
    },
    "cuke": {
      "dependsOn": ["build"],
      "inputs": ["**/*.feature", "**/*.ts", ":!:src/*.test.ts", ":!:src/**/*.test.ts", "cucumber.cjs"],
      "outputs": [],
      "outputMode": "new-only"
    },
    "doc": {
      "dependsOn": ["build"],
      "inputs": [
        "*.md",
        "**/*.md",
        "src/*.ts",
        "src/**/*.ts",
        ":!:src/*.test.ts",
        ":!:src/**/*.test.ts",
        "text-run.yml"
      ],
      "outputMode": "new-only"
    },
    "fix": {
      "outputs": [],
      "outputMode": "new-only"
    },
    "lint": {
      "outputs": [],
      "outputMode": "new-only"
    },
    "test": {
      "dependsOn": ["^test", "build", "unit", "cuke", "doc"],
      "outputs": [],
      "inputs": ["src/*.ts", "src/**/*.ts"],
      "outputMode": "new-only"
    },
    "unit": {
      "dependsOn": ["^test"],
      "inputs": ["src/**/*.ts", ".mocharc.yml"],
      "outputs": [],
      "outputMode": "new-only"
    }
  },
  "globalDependencies": ["tsconfig.json", "package.json", "yarn.lock"]
}
